#include <stdio.h>
int main(int argc, char const *argv[])
{
    int a;
    printf("Input\nEnter the number you want multiplication table of:");
    scanf("%d",&a);

    printf("Output:\nTable of:\n");
    
    printf("%d*1=%d\n",a,a*1);
    printf("%d*2=%d\n",a,a*2);
    printf("%d*3=%d\n",a,a*3);
    printf("%d*4=%d\n",a,a*4);
    printf("%d*5=%d\n",a,a*5);
    printf("%d*6=%d\n",a,a*6);
    printf("%d*7=%d\n",a,a*7);
    printf("%d*8=%d\n",a,a*8);
    printf("%d*9=%d\n",a,a*9);
    printf("%d*1=%d\n",a,a*10);
    

    return 0;
}
